{
  "name": "keyv-duckdb",
  "version": "0.0.0",
  "description": "DuckDB storage adapter for Keyv with AES-256-GCM encryption support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmalakoff/keyv-duckdb.git"
  },
  "bugs": {
    "url": "https://github.com/kmalakoff/keyv-duckdb/issues"
  },
  "homepage": "https://github.com/kmalakoff/keyv-duckdb#readme",
  "keywords": [
    "keyv",
    "duckdb",
    "storage",
    "encryption",
    "cache",
    "key-value"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && tsds build",
    "format": "biome check --write --unsafe",
    "test": "node --test 'test/**/*.test.ts'",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "prepublishOnly": "npm run build",
    "version": "tsds version"
  },
  "dependencies": {
    "duckdb": "1.4.0",
    "signal-exit": "^4.1.0"
  },
  "peerDependencies": {
    "keyv": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/node": "^24.6.2",
    "keyv": "^5.5.3",
    "ts-dev-stack": "^1.18.8",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16"
  },
  "tsds": {
    "source": "src/index.ts"
  },
  "depcheck": {
    "ignores": [
      "@biomejs/biome",
      "ts-dev-stack",
      "typescript"
    ]
  }
}
